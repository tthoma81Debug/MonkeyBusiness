    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "nodemon": "^3.0.1"
  },
  "dependencies": {
    "@react-native-community/masked-view": "^0.1.11",
