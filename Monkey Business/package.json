{
  "name": "Monkey Business Web App",
  "version": "0.1.0",
  "description": "React",
  "main": "server/mbServer.js",
  "type": "module",
  "private": true,
  "scripts": {
    "clean": "rm public/*.map public/mbClient.js public/mbClient.css",
    "client": "esbuild --bundle --minify --target=es6 --outdir=public client/main.jsx",
    "dev": "concurrently \"npm run client:dev\" \"npm run server:dev\"",
    "client:dev": "esbuild --watch --bundle --sourcemap --target=es6 --outdir=public client/main.jsx",
    "server:dev": "nodemon --watch server dev",
    "server:prod": "node server/mbServer.js prod",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
